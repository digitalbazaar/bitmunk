{
   "_id_": "bitmunk.apps.bpe",
   "_version_": "Monarch Config 3.0",
   "_group_": "after defaults",
   "_append_": {
      "monarch.app.Kernel": {
         "appPath": "@BITMUNK_DIR@/dist/modules/apps/@LIB_PREFIX@bitmunk.@DYNAMIC_LIB_EXT@"
      },
      "monarch.app.Config": {
         "appConfigs": [
            "@BITMUNK_DIR@/configs/dev-bpe.config"
         ]
      }
   },
   "_merge_": {
      "monarch.app.Core": {
         "home": "@BITMUNK_HOME_DIR@"
      },
      "monarch.app.Logging": {
         "color": true,
         "level": "max"
      }
   }
}
